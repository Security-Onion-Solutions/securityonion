       "dynamic":false,
       "date_detection":false,
       "dynamic_templates": [
       {
          "ip_address": {
            "match_mapping_type": "string",
            "path_match": "*.ip",
            "mapping": {
              "type": "ip",
              "fields" : {
                "keyword" : {
                  "ignore_above" : 45,
                  "type" : "keyword"
                }
              }

            }
          }
